    var url = 'http://api.cirrusimage.net/collections.json'
    $.getJSON("http://api.cirrusimage.net/collections/current_exhibition.json?callback=?",
        function(data){
        $.each(data, function(i,item){
            $("#images").append(data[i]);
            //if ( i == 3 ) return false;
        });
    });
