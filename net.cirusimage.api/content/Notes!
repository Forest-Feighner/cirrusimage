    RewriteRule  ^collections.html$ http://api.cirrusimage.netimg.php?context=htmlCollections	[P,L]
    RewriteRule  ^collections.xhtml$ http://api.cirrusimage.netimg.php?context=xhtmlCollections	[P,L]
    RewriteRule  ^collections.json$ http://api.cirrusimage.netimg.php?context=jsonCollections	[QSA,P,L]
    RewriteRule  ^collections/(.*).xhtml$ http://api.cirrusimage.netimg.php?context=xhtmlCollectionsFiles&gallery=$1	[P,L]
    RewriteRule  ^collections/(.*).html$ http://api.cirrusimage.netimg.php?context=htmlCollectionsFiles&gallery=$1	[P,L]
    RewriteRule  ^collections/(.*).json$ http://api.cirrusimage.netimg.php?context=jsonCollectionsFiles&gallery=$1	[QSA,P,L]

    RewriteRule  (.*)/(.*).xhtml$ http://api.cirrusimage.netimg.php?context=xhtml&gallery=$1&image=$2.jpg	[P,L]
    RewriteRule  (.*)/(.*).html$ http://api.cirrusimage.netimg.php?context=html&gallery=$1&image=$2.jpg	[P,L]

    RewriteRule  ^(.*)/(.*).json$ http://api.cirrusimage.netimg.php?context=json&gallery=$1&image=$2.jpg	[QSA,P,L]
    RewriteRule  ^(.*)/(.*).thumbnail.jpg$ http://api.cirrusimage.netimg.php?context=thumbnail&gallery=$1&image=$2.jpg	[P,L]
    RewriteRule  ^(.*)/(.*).base64$ http://api.cirrusimage.netimg.php?context=base64&gallery=$1&image=$2.jpg	[P,L]

    RewriteRule  ^image/(.*)$  http://api.cirrusimage.netimage/$1	[R,L]


image sitemap.xml
http://www.google.com/support/webmasters/bin/answer.py?answer=178636
<?xml version="1.0" encoding="UTF-8"?>
 <urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
  xmlns:image="http://www.google.com/schemas/sitemap-image/1.1">
 <url>
   <loc>http://example.com/sample.html</loc>
   <image:image>
     <image:loc>http://example.com/image.jpg</image:loc>
   </image:image>
   <image:image>
     <image:loc>http://example.com/photo.jpg</image:loc>
   </image:image>
 </url>
</urlset>



